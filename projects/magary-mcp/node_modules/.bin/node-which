#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules/which/bin/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules/which/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/node_modules:/mnt/c/Users/sebas/Documents/GitHub/node_modules:/mnt/c/Users/sebas/Documents/node_modules:/mnt/c/Users/sebas/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules/which/bin/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules/which/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/node_modules:/mnt/c/Users/sebas/Documents/GitHub/node_modules:/mnt/c/Users/sebas/Documents/node_modules:/mnt/c/Users/sebas/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/sebas/Documents/GitHub/magary/projects/magary-mcp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../which/bin/node-which" "$@"
else
  exec node  "$basedir/../which/bin/node-which" "$@"
fi
